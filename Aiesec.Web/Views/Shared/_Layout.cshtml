    <script>

    function gettoken() {
        var token = '@Html.AntiForgeryToken()';
        token = $(token).val();
      return token;
    }

$(document.body).on('click', '[ajax-add]', function (e) {
    e.preventDefault();
    var element = $(this);
    $.ajax({
        url: $(this).attr("href"),
        type: "get",
        headers: {
            'RequestVerificationToken': gettoken()
        },
        async: true,
        success: function (result) {
            $(`#${element.attr("ajax-add")} .modal-content`).html(result);
            $(`#${element.attr("ajax-add")}`).modal('show');
        },
        error: function (jqXHR, ajaxOptions, thrownError) {
            if (jqXHR.responseJSON == null)
                 Swal.fire({
                    position: "top-right",
                    icon: "error",
                    title: "@_sharedLocalizer.ResourceCantBeRetrieved",
                    showConfirmButton: false,
                    timer: 1500
                });
        }
    });
});

        function updateRow(encryptedId, model) {
            var table = $("#datatable");
            console.log(model);
            if (table !== undefined) {
                var targetRow = $(`#${encryptedId}`);
                table.DataTable().row(targetRow).data(model);
            }
        }

        $(document.body).on("click", "[ajax-change-active-status]", function (e) {
            e.preventDefault();
            var encryptedId = $(this).attr("encryptedId");
            $.ajax({
                url: $(this).attr("href"),
                type: "get",
                headers: {
                    "RequestVerificationToken": gettoken()
                },
                async: true,
                success: function (data, textStatus, jqXHR) {
                    updateRow(encryptedId, data.model);
                    Swal.fire({
                        position: "top-right",
                        icon: "success",
                        title: data.message,
                        showConfirmButton: false,
                        timer: 1500
                    });
                },
                error: function (jqXHR, ajaxOptions, thrownError) {
                    Swal.fire({
                        position: "top-right",
                        icon: "error",
                        title: jqXHR.responseJSON.message,
                        showConfirmButton: false,
                        timer: 1500
                    });
                }
            });
        });

        $(document.body).on('click', '[ajax-delete]', function (e) {
            e.preventDefault();
            var encryptedId = $(this).attr("encryptedId");
            var element = $(this);
            var table = $("#datatable").DataTable();
            Swal.fire({
                title: '@_sharedLocalizer.AreYouSure',
                type: 'warning',
                showCancelButton: true,
                confirmButtonText: '@_sharedLocalizer.Delete',
                confirmButtonColor: "#d33",
               cancelButtonText: '@_sharedLocalizer.Cancel'
            }).then(function (result) {
                if (result.value) {
                    $.ajax({
                        url: element.attr("href"),
                        type: "delete",
                        headers: {
                            "RequestVerificationToken": gettoken()
                        },
                        async: true,
                        success: function (data, textStatus, jqXHR) {
                            table.row(`#${encryptedId}`).remove().draw();
                            Swal.fire({
                                position: "top-right",
                                icon: "success",
                                title: "@_sharedLocalizer.SuccDelete",
                                showConfirmButton: false,
                                timer: 1500
                            });
                        },
                        error: function (jqXHR, ajaxOptions, thrownError) {
                            Swal.fire({
                                position: "top-right",
                                icon: "error",
                                title: "@_sharedLocalizer.ErrDelete",
                                showConfirmButton: false,
                                timer: 1500
                            });
                        }
                    });
                    $.get(element.attr('href'), function (response) {
                        if (response.success) {
                            toastr.success(response.message);
                            if (typeof table !== "undefined" && table)
                                table.ajax.reload();
                            return;
                        }
                        toastr.error(response.message);
                    });
                }
            });
        });
    </script>
